{
  "name": "handy-redis",
  "version": "0.0.0-development",
  "description": "A wrapper around node_redis with Promise and TypeScript support.",
  "main": "dist/src/index.js",
  "files": [
    "dist/src"
  ],
  "types": "dist/src/index",
  "scripts": {
    "clean": "shx rm -rf {dist,src/generated,test/generated}",
    "compile": "tsc --project .",
    "prebuild": "ts-node scripts/generate-client",
    "build": "tsc --project .",
    "lint": "tslint **/*.ts",
    "pretest": "npm run build",
    "test": "ava",
    "posttest": "npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "coverage": "nyc ava && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmkal/handy-redis.git"
  },
  "keywords": [
    "redis"
  ],
  "author": "mmkal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmkal/handy-redis/issues"
  },
  "homepage": "https://github.com/mmkal/handy-redis#readme",
  "ava": {
    "files": [
      "dist/test",
      "**/__tests__/*.js"
    ],
    "serial": true
  },
  "dependencies": {
    "@types/redis": "^0.12.36",
    "redis": "^2.7.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.63",
    "@types/node": "^7.0.14",
    "@types/shelljs": "^0.7.2",
    "@types/yamljs": "^0.2.30",
    "ava": "^0.23.0",
    "coveralls": "^2.13.1",
    "lodash": "^4.17.4",
    "nyc": "^10.3.0",
    "semantic-release": "^6.3.2",
    "shelljs": "^0.7.7",
    "shx": "^0.2.2",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.3.2",
    "yamljs": "^0.2.10"
  }
}
