{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "array-type": false,
        "no-console": [
            true
        ],
        "no-angle-bracket-type-assertion": false,
        "unified-signatures": false,
        "variable-name": [
            "allow-leading-underscore"
        ],
        "ban-types": false,
        "arrow-parens": false,
        "max-line-length": [
            180
        ],
        "member-ordering": false,
        "interface-name": [
            false
        ],
        "max-classes-per-file": [
            false
        ],
        "member-access": [
            false
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "no-inferrable-types": [
            true
        ],
        "trailing-comma": [
            true
        ],
        "no-empty": false
    }
}